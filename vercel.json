{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.py"
        }
    ]
}





{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}
